  "scripts": {
    "start": "npm run-script clean:project && webpack --env=dev --mode=development --progress --profile && concurrently \"webpack serve --env=dev --mode=development\" \"bundle exec jekyll serve\"",
    "build:optim": "npm run-script clean:project && cross-env NODE_ENV=production webpack --env=prod --mode=production --progress --profile && webpack --env=optim  --progress --profile && cross-env JEKYLL_ENV=production bundle exec jekyll build --config _config.yml,_config_prod.yml",
    "build": "npm run-script clean:project && cross-env NODE_ENV=production webpack --env=prod --mode=production --progress --profile && cross-env JEKYLL_ENV=production bundle exec jekyll build --config _config.yml,_config_prod.yml",
    "serve:dist": "http-server _site",
    "clean:project": "rimraf ./_site ./assets"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
